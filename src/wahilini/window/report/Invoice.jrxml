	</parameter>
	<queryString>
		<![CDATA[SELECT
     invoice.`id` AS invoice_id,
     invoice.`inv_no` AS invoice_inv_no,
     invoice.`grand_total` AS invoice_grand_total,
     invoice.`total_discount` AS invoice_total_discount,
     invoice.`net_total` AS invoice_net_total,
     invoice.`inv_date` AS invoice_inv_date,
     invoice.`paid_amount` AS invoice_paid_amount,
     invoice.`balance_amount` AS invoice_bal_amount,
     invoice.`inv_time` AS invoice_inv_time,
     employee.`name` AS employee_name,
     invoiceitems.`unit_price` AS invoiceitems_unit_price,
     invoiceitems.`sub_total` AS invoiceitems_sub_total,
     invoiceitems.`qty` AS invoiceitems_qty,
     invoiceitems.`discount` AS invoiceitems_discount,
     item.`Item_Name` AS item_Item_Name,
     item.`Retail_Price` AS item_Retail_Price,
     customer.`name` AS customer_name,
     invoice.`service_charge` AS invoice_service_charge,
     invoice.`pay_type` AS invoice_pay_type
FROM
     `invoice` invoice INNER JOIN `employee` employee ON invoice.`employee_id` = employee.`id`
     INNER JOIN `invoice_items` invoiceitems ON invoice.`id` = invoiceitems.`Invoice_id`
     INNER JOIN `customer` customer ON invoice.`customer_id` = customer.`id`
     INNER JOIN `item` item ON invoiceitems.`Item_Id` = item.`Id`
WHERE
     invoice.`id` = $P{invoiceId}]]>
	</queryString>
